<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.xc.baseproject.basefeature">

    <application
        android:icon="@drawable/ic_android_24dp"
        tools:replace="android:icon">

        <activity android:name=".ui.testmain.MainActivity">
            <intent-filter android:order="1">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="xc.com"
                    android:pathPattern="/.*"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity android:name="com.xc.baseproject.basefeature.testtuling.TulingActivity" />
        <activity
            android:name="com.xc.Main996Activity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="com.xc.AddNewItemActivity" />
    </application>

</manifest>